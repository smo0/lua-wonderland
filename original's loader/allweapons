            ui.set_visible(weapon_name[1]["Doub-Minimum hit chance"], true)
            ui.set_visible(weapon_name[1]["Doub-Minimum damage"], true)
        end
    end
    client.set_event_callback("paint_ui", ui_set_visible)
    
