            ui.set_visible(weapon_name[14]["Mini-Minimum damage"], true)
            ui.set_visible(weapon_name[14]["Doub-lable"], true)
            ui.set_visible(weapon_name[14]["Doub-Target hitbox"], true)
            ui.set_visible(weapon_name[14]["Doub-Multi-point"], true)
            ui.set_visible(weapon_name[14]["Doub-Multi-point scale"], true)
            ui.set_visible(weapon_name[14]["Doub-Avoid unsafe hitboxes"], true)
            ui.set_visible(weapon_name[14]["Doub-Minimum hit chance"], true)
            ui.set_visible(weapon_name[14]["Doub-Minimum damage"], true)
        end
    end
    client.set_event_callback("paint_ui", ui_set_visible13)
    
    function ui_set_value_set()
        for i =1,14 do
                ui.set(weapon_name[i]["Visi-Target hitbox"], "Head")
                ui.set(weapon_name[i]["Auto-Target hitbox"], "Head")
                ui.set(weapon_name[i]["Mini-Target hitbox"], "Head")
                ui.set(weapon_name[i]["Doub-Target hitbox"], "Head")
        end
    end
    ui_set_value_set()
    
    function visible_autowall()
        local me = entity.get_local_player()
        local a, b, c = client.eye_position()
        local enemies = entity.get_players(true)
        for i=1, #enemies do
            g = 0
            local x, y, z = entity.hitbox_position(enemies[i], 0)
            local wx, wy = renderer.world_to_screen(x, y, z)
            if wx ~= nil then
                local help, thisisaids = client.trace_line(me, a, b, c, x, y, z)
    
                if thisisaids == enemies[i] or help == 1 then
                    g = 1
                end
            else
                g = 0
            end
        end
    end
    client.set_event_callback("paint", visible_autowall)
    
    local target_selection_ref = ui.reference("Rage", "Aimbot", "Target selection")
    local target_hitbox_ref = ui.reference("Rage", "Aimbot", "Target hitbox")
    local multi_point_ref,multi_point_mode_ref = ui.reference("Rage", "Aimbot", "Multi-point")
    local multi_point_scale_ref = ui.reference("Rage", "Aimbot", "Multi-point scale")
    local prefer_safe_point_ref =ui.reference("Rage","Aimbot", "Prefer safe point")
    local Avoid_unsafe_hitboxes_ref = ui.reference("Rage", "Aimbot", "Avoid unsafe hitboxes")
    local Automatic_scope_ref =ui.reference("Rage", "Aimbot", "Automatic scope")
    local Force_body_aim_on_peek_ref = ui.reference("Rage", "Other","Force body aim on peek")
    local quick_stop_ref = ui.reference("Rage", "Other", "Quick stop options")
    local minimum_hit_chance_ref = ui.reference("Rage", "Aimbot", "Minimum hit chance")
    local minimum_damage_ref = ui.reference("Rage", "Aimbot", "Minimum damage")
    
    function set_value_weapon()
        local ME = entity.get_local_player()
        local weapons_all_name = entity.get_player_weapon(ME)
        local res_name = entity.get_classname(weapons_all_name)
        return res_name
    end
    client.set_event_callback("setup_command", set_value_weapon)
    
    
    function wep_set_value()
        if set_value_weapon() == "CWeaponHKP2000" or set_value_weapon() == "CWeaponGlock"then
            ui.set(target_selection_ref,ui.get(weapon_name[1]["Target selection"]))
            ui.set(multi_point_mode_ref,ui.get(weapon_name[1]["Multi-point_mode"]))
            ui.set(prefer_safe_point_ref,ui.get(weapon_name[1]["Prefer safe point"]))
            ui.set(quick_stop_ref,ui.get(weapon_name[1]["quick stop options"]))
            ui.set(Automatic_scope_ref,ui.get(weapon_name[1]["Automatic scope"]))
            ui.set(Force_body_aim_on_peek_ref,ui.get(weapon_name[1]["Force body aim on peek"]))
            if  damage[0] == 1 then
                ui.set(target_hitbox_ref,ui.get(weapon_name[1]["Mini-Target hitbox"]))
                ui.set(multi_point_ref,ui.get(weapon_name[1]["Mini-Multi-point"]))
                ui.set(multi_point_scale_ref,ui.get(weapon_name[1]["Mini-Multi-point scale"]))
                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[1]["Mini-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[1]["Mini-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[1]["Mini-Minimum hit chance"]))
        elseif damage[0] == 2 then
                ui.set(target_hitbox_ref,ui.get(weapon_name[1]["Doub-Target hitbox"]))
                ui.set(multi_point_ref,ui.get(weapon_name[1]["Doub-Multi-point"]))
                ui.set(multi_point_scale_ref,ui.get(weapon_name[1]["Doub-Multi-point scale"]))
                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[1]["Doub-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[1]["Doub-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[1]["Doub-Minimum hit chance"]))
        else
            if g==0 then
                ui.set(target_hitbox_ref,ui.get(weapon_name[1]["Auto-Target hitbox"]))
                ui.set(multi_point_ref,ui.get(weapon_name[1]["Auto-Multi-point"]))
                ui.set(multi_point_scale_ref,ui.get(weapon_name[1]["Auto-Multi-point scale"]))
                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[1]["Auto-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[1]["Auto-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[1]["Auto-Minimum hit chance"]))
            else
                ui.set(target_hitbox_ref,ui.get(weapon_name[1]["Visi-Target hitbox"]))
                ui.set(multi_point_ref,ui.get(weapon_name[1]["Visi-Multi-point"]))
                ui.set(multi_point_scale_ref,ui.get(weapon_name[1]["Visi-Multi-point scale"]))
                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[1]["Visi-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[1]["Visi-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[1]["Visi-Minimum hit chance"]))
            end
        end
    end
    end
    client.set_event_callback("setup_command", wep_set_value)
    
