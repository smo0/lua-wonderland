--table add
    weapon_fire ={}
    weapon_fire[1] = "USP/Glock"
    weapon_fire[2] = "R8/Deagle"
    weapon_fire[3] = "Dual Elites"
    weapon_fire[4] = "AK47"
    weapon_fire[5] = "Submachine"
    weapon_fire[6] = "Scout"
    weapon_fire[7] = "AWP"
    weapon_fire[8] = "Scar-20"
    weapon_fire[9] = "G3SG1"
    weapon_fire[10] = "Pistol"
    weapon_fire[11] = "Shotgun"
    weapon_fire[12] = "Machine"
    weapon_fire[13] = "Rifle"
    weapon_fire[14] = "Zeus"
    
    
    
    damage = {}
    damage[0] = 0
    
    
    ui.new_label("Lua", "B", "---------Original 2.0 All Weapons Lua---------")
    
    local weapon_name_combobox = ui.new_combobox("Lua", "B", "Weapons",  "USP/Glock","R8/Deagle","Dual Elites","AK47","Submachine","Scout","AWP","Scar-20","G3SG1","Pistol","Shotgun","Machine","Rifle","Zeus")
    
    local mini_hotkey = ui.new_hotkey("Lua", "B", "Minimum Damage Hotkey Bind ->")
    local dt_enable_ref,dt_hotkey_ref = ui.reference("Rage", "Other", "Double tap")
    ui.set(mini_hotkey,"Toggle")
    client.set_event_callback("paint_ui", function()
        if ui.get(mini_hotkey)then
            damage[0] = 1
        elseif ui.get(dt_hotkey_ref) and ui.get(dt_enable_ref) then
            damage[0] = 2
        else
            damage[0] = 0
        end
    end)
    
    function paint_damage()
        local x,y = client.screen_size()
        local x1,y1 = x/2,y/2-20
        local damage_value = ui.get(ui.reference("Rage","Aimbot", "Minimum damage"))
        if damage[0]==0 then
            renderer.text(x1, y1, 	0,255,127, 255, "cb", 0, "Damage :"..damage_value)
        elseif damage[0]==1 then
            renderer.text(x1, y1, 	0,255,255, 255, "cb", 0, "Minimum :"..damage_value)
        elseif damage[0]==2 then
            renderer.text(x1, y1, 	255,0,0, 255, "cb", 0, "Double tap :"..damage_value)
        end
    end
    client.set_event_callback("paint",paint_damage)
    
    local IQ_lua = ui.new_checkbox("Lua", "B", "IQ")
    
    function paint_hitchance()
        local x,y = client.screen_size()
        local x1,y1 = x/2,y/2-30
        local hit_value = ui.get(ui.reference("Rage","Aimbot", "Minimum hit chance"))
        if ui.get(IQ_lua)then
            renderer.text(x1, y1, 	255,105,180, 255, "cb", 0, "IQ:"..hit_value.."%")
        else
        renderer.text(x1, y1, 	255,105,180, 255, "cb", 0, "HC:"..hit_value.."%")
        end
    end
    client.set_event_callback("paint",paint_hitchance)
    
    
    
    
    
    local rage_enable_ref,hotkey_ref = ui.reference("Rage", "Aimbot", "Enabled")
    ui.set(rage_enable_ref,true)
    ui.set(hotkey_ref,"Always on")
    
    
    local delay_shot = ui.new_hotkey("Lua", "B", "Delay shot")
    ui.set(delay_shot,"Toggle")
    local delayshot_ref = ui.reference("Rage", "Other", "Delay shot")
    client.set_event_callback("paint_ui", function()
        if ui.get(delay_shot)then
            ui.set(delayshot_ref,true)
        else
            ui.set(delayshot_ref,false)
        end
    end)
    
    function delay_shot_paint()
        if ui.get(delay_shot)then
        local x,y = client.screen_size()
        local x1,y1 = 60,y/2-40
        renderer.text(x1, y1, 	255,165,0, 255, "c+", 0, "Delay Shot")
        end
    end
    client.set_event_callback("paint", delay_shot_paint)
    
    
    
    
    local Fov_sli = ui.new_slider("Lua", "B", "Maximum FOV", 1, 180, 180, true, "Â°")
    local FOV_ref = ui.reference("Rage", "Aimbot", "Maximum FOV")
    client.set_event_callback("paint_ui", function()
        ui.set(FOV_ref,ui.get(Fov_sli))
    end)
    
    local Reduce_aim_step = ui.new_checkbox("Lua", "B", "Reduce aim step")
    local reduce_ref = ui.reference("Rage","Aimbot", "Reduce aim step")
    ui.set_callback(Reduce_aim_step, function()
        if ui.get(Reduce_aim_step)then
            ui.set(reduce_ref,true)
        else
            ui.set(reduce_ref,false)
        end
    end)
    
    
    
    local automatic_fire = ui.new_checkbox("Lua", "B", "Automatic fire")
    local automatic_fire_ref = ui.reference("Rage", "Aimbot","Automatic fire")
    ui.set_callback(automatic_fire, function()
        if ui.get(automatic_fire)then
            ui.set(automatic_fire_ref,true)
        else
            ui.set(automatic_fire_ref,false)
        end
    end)
    
    local automatic_penetration = ui.new_checkbox("Lua", "B", "Automatic penetration")
