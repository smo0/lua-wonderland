        if ui.get(automatic_penetration)then
            ui.set(automatic_penetration_ref,true)
        else
            ui.set(automatic_penetration_ref,false)
        end
    end)
    
    local Silent_aim = ui.new_checkbox("Lua", "B", "Silent aim")
    local silent_ref = ui.reference("Rage", "Aimbot", "Silent aim")
    ui.set_callback(Silent_aim, function()
        if ui.get(Silent_aim)then
            ui.set(silent_ref,true)
        else
            ui.set(silent_ref,false)
        end
    end)
    
    local Log_miss = ui.new_checkbox("Lua", "B","Log misses due to spread")
    local log_miss_ref = ui.reference("Rage", "Aimbot", "Log misses due to spread")
    ui.set_callback(Log_miss, function()
        if ui.get(Log_miss)then
            ui.set(log_miss_ref,true)
        else
            ui.set(log_miss_ref,false)
        end
    end)
    
    
    
    local Remov_recoil = ui.new_checkbox("Lua", "B","Remove recoil")
    local remov_re_ref = ui.reference("Rage", "Other","Remove recoil")
    ui.set_callback(Remov_recoil, function()
        if ui.get(Remov_recoil)then
            ui.set(remov_re_ref,true)
        else
            ui.set(remov_re_ref,false)
        end
    end)
    
    local accuracy = ui.new_combobox("Lua", "B", "Accuracy boost","Off","Low","Medium","High","Maximum")
    local acc_ref = ui.reference("Rage", "Other", "Accuracy boost")
    ui.set_callback(accuracy, function()
        ui.set(acc_ref,ui.get(accuracy))
    end)
    
    local quick_stop_ref = ui.reference("Rage","Other","Quick stop")
    client.set_event_callback("paint_ui", function()
        if ui.get(quick_stop_ref)then
        else
        ui.set(quick_stop_ref,true)
        end
    end)
    
    color = {}
    color[0] = 255
    end_1 = 0
    function copyright()
        local x,y = client.screen_size()
        local a = color[0]
        renderer.text(75, y-7, 0,255,255, a, "c", 0, "Lua Copyright@2021 By Original")
        if a == 0 then
            end_1 = 1
            elseif a== 255 then
            end_1 = 0
            end
            if end_1 == 0 then
                color[0]= a-1
            elseif end_1 == 1 then
                color[0] = a+1
            end
    end
    client.set_event_callback("paint_ui", copyright)
    
    
    
    
    weapon_name = {}
    function ui_add()
        for i=1,14 do
            weapon_name[i]={
                ["Target selection"]=ui.new_combobox("Lua", "B", "["..weapon_fire[i].."]".."Target selection", "Cycle","Cycle (2x)","Near crosshair","Highest damage","Lowest ping","Best K/D ration","Best hit chance"),
                ["Multi-point_mode"] = ui.new_combobox("Lua", "B", "["..weapon_fire[i].."]".."Multi-point Mode", "Low","Medium","High"),
                ["Prefer safe point"] = ui.new_checkbox("Lua", "B", "["..weapon_fire[i].."]".."Prefer safe point"),
                ["quick stop options"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Quick stop options ","Early","Slow motion","Duck","Fake duck","Move between shots","Ignore molotov","Taser"),
                ["Automatic scope"] = ui.new_checkbox("Lua", "B", "["..weapon_fire[i].."]".."Automatic scope"),
                ["Force body aim on peek"] = ui.new_checkbox("Lua","B", "["..weapon_fire[i].."]".."Force body aim on peek"),
                
                ["Visi-lable"] = ui.new_label("Lua", "B",  "["..weapon_fire[i].."]".."---".."Visible".."---"),
                ["Visi-Target hitbox"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Visible-Target hitbox", "Head","Chest","Stomach","Arms","Legs","Feet"),
                ["Visi-Multi-point"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Visible-Multi-point", "Head","Chest","Stomach","Arms","Legs","Feet"),
                ["Visi-Multi-point scale"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Visible-Multi-point scale",25, 100, 25, true, "%"),
                ["Visi-Avoid unsafe hitboxes"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Visible-Avoid unsafe hitboxes", "Head","Chest","Stomach","Arms","Legs"),
                ["Visi-Minimum hit chance"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Visible-Minimum hit chance", 1, 100, 1, true,"%"),
                ["Visi-Minimum damage"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Visible-Minimum damage", 1, 126, 1, true,"HP"),
    
                
                ["Auto-lable"] = ui.new_label("Lua", "B",  "["..weapon_fire[i].."]".."---".."Autowall".."---"),
                ["Auto-Target hitbox"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Autowall-Target hitbox", "Head","Chest","Stomach","Arms","Legs","Feet"),
                ["Auto-Multi-point"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Autowall-Multi-point", "Head","Chest","Stomach","Arms","Legs","Feet"),
                ["Auto-Multi-point scale"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Autowall-Multi-point scale",25, 100, 25, true, "%"),
                ["Auto-Avoid unsafe hitboxes"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Autowall-Avoid unsafe hitboxes", "Head","Chest","Stomach","Arms","Legs"),
                ["Auto-Minimum hit chance"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Autowall-Minimum hit chance", 1, 100, 1, true,"%"),
                ["Auto-Minimum damage"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Autowall-Minimum damage", 1, 126, 1, true,"HP"),
    
                ["Mini-lable"] = ui.new_label("Lua", "B",  "["..weapon_fire[i].."]".."---".."Minimum".."---"),
                ["Mini-Target hitbox"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Minimum-Target hitbox", "Head","Chest","Stomach","Arms","Legs","Feet"),
                ["Mini-Multi-point"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Minimum-Multi-point", "Head","Chest","Stomach","Arms","Legs","Feet"),
                ["Mini-Multi-point scale"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Minimum-Multi-point scale",25, 100, 25, true, "%"),
                ["Mini-Avoid unsafe hitboxes"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Minimum-Avoid unsafe hitboxes", "Head","Chest","Stomach","Arms","Legs"),
                ["Mini-Minimum hit chance"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Minimum-Minimum hit chance", 1, 100, 1, true,"%"),
                ["Mini-Minimum damage"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Minimum-Minimum damage", 1, 126, 1, true,"HP"),
    
                ["Doub-lable"] = ui.new_label("Lua", "B",  "["..weapon_fire[i].."]".."---".."Double tap".."---"),
                ["Doub-Target hitbox"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Double tap-Target hitbox", "Head","Chest","Stomach","Arms","Legs","Feet"),
                ["Doub-Multi-point"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Double tap-Multi-point", "Head","Chest","Stomach","Arms","Legs","Feet"),
                ["Doub-Multi-point scale"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Double tap-Multi-point scale",25, 100, 25, true, "%"),
                ["Doub-Avoid unsafe hitboxes"] = ui.new_multiselect("Lua", "B", "["..weapon_fire[i].."]".."Double tap-Avoid unsafe hitboxes", "Head","Chest","Stomach","Arms","Legs"),
                ["Doub-Minimum hit chance"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Double tap-Minimum hit chance", 1, 100, 1, true,"%"),
                ["Doub-Minimum damage"] = ui.new_slider("Lua", "B", "["..weapon_fire[i].."]".."Double tap-Minimum damage", 1, 126, 1, true,"HP"),
            }
        end
    end
    ui_add()
    
    
    
    function ui_set_visible()
        if ui.get(weapon_name_combobox)=="USP/Glock"then
            for i=2,14 do
                ui.set_visible(weapon_name[i]["Target selection"], false)
                ui.set_visible(weapon_name[i]["Multi-point_mode"], false)
                ui.set_visible(weapon_name[i]["Prefer safe point"], false)
                ui.set_visible(weapon_name[i]["quick stop options"], false)
                ui.set_visible(weapon_name[i]["Automatic scope"], false)
                ui.set_visible(weapon_name[i]["Force body aim on peek"], false)
                ui.set_visible(weapon_name[i]["Visi-lable"], false)
                ui.set_visible(weapon_name[i]["Visi-Target hitbox"], false)
                ui.set_visible(weapon_name[i]["Visi-Multi-point"], false)
                ui.set_visible(weapon_name[i]["Visi-Multi-point scale"], false)
                ui.set_visible(weapon_name[i]["Visi-Avoid unsafe hitboxes"], false)
                ui.set_visible(weapon_name[i]["Visi-Minimum hit chance"], false)
                ui.set_visible(weapon_name[i]["Visi-Minimum damage"], false)
                ui.set_visible(weapon_name[i]["Auto-lable"], false)
                ui.set_visible(weapon_name[i]["Auto-Target hitbox"], false)
                ui.set_visible(weapon_name[i]["Auto-Multi-point"], false)
                ui.set_visible(weapon_name[i]["Auto-Multi-point scale"], false)
                ui.set_visible(weapon_name[i]["Auto-Avoid unsafe hitboxes"], false)
                ui.set_visible(weapon_name[i]["Auto-Minimum hit chance"], false)
                ui.set_visible(weapon_name[i]["Auto-Minimum damage"], false)
                ui.set_visible(weapon_name[i]["Mini-lable"], false)
                ui.set_visible(weapon_name[i]["Mini-Target hitbox"], false)
                ui.set_visible(weapon_name[i]["Mini-Multi-point"], false)
                ui.set_visible(weapon_name[i]["Mini-Multi-point scale"], false)
                ui.set_visible(weapon_name[i]["Mini-Avoid unsafe hitboxes"], false)
                ui.set_visible(weapon_name[i]["Mini-Minimum hit chance"], false)
                ui.set_visible(weapon_name[i]["Mini-Minimum damage"], false)
                ui.set_visible(weapon_name[i]["Doub-lable"], false)
                ui.set_visible(weapon_name[i]["Doub-Target hitbox"], false)
                ui.set_visible(weapon_name[i]["Doub-Multi-point"], false)
                ui.set_visible(weapon_name[i]["Doub-Multi-point scale"], false)
                ui.set_visible(weapon_name[i]["Doub-Avoid unsafe hitboxes"], false)
                ui.set_visible(weapon_name[i]["Doub-Minimum hit chance"], false)
                ui.set_visible(weapon_name[i]["Doub-Minimum damage"], false)
            end
            ui.set_visible(weapon_name[1]["Target selection"], true)
            ui.set_visible(weapon_name[1]["Multi-point_mode"], true)
            ui.set_visible(weapon_name[1]["Prefer safe point"], true)
            ui.set_visible(weapon_name[1]["quick stop options"], true)
            ui.set_visible(weapon_name[1]["Automatic scope"], true)
            ui.set_visible(weapon_name[1]["Force body aim on peek"], true)
            ui.set_visible(weapon_name[1]["Visi-lable"], true)
            ui.set_visible(weapon_name[1]["Visi-Target hitbox"], true)
            ui.set_visible(weapon_name[1]["Visi-Multi-point"], true)
            ui.set_visible(weapon_name[1]["Visi-Multi-point scale"], true)
            ui.set_visible(weapon_name[1]["Visi-Avoid unsafe hitboxes"], true)
            ui.set_visible(weapon_name[1]["Visi-Minimum hit chance"], true)
            ui.set_visible(weapon_name[1]["Visi-Minimum damage"], true)
            ui.set_visible(weapon_name[1]["Auto-lable"], true)
            ui.set_visible(weapon_name[1]["Auto-Target hitbox"], true)
            ui.set_visible(weapon_name[1]["Auto-Multi-point"], true)
            ui.set_visible(weapon_name[1]["Auto-Multi-point scale"], true)
            ui.set_visible(weapon_name[1]["Auto-Avoid unsafe hitboxes"], true)
            ui.set_visible(weapon_name[1]["Auto-Minimum hit chance"], true)
            ui.set_visible(weapon_name[1]["Auto-Minimum damage"], true)
            ui.set_visible(weapon_name[1]["Mini-lable"], true)
            ui.set_visible(weapon_name[1]["Mini-Target hitbox"], true)
            ui.set_visible(weapon_name[1]["Mini-Multi-point"], true)
