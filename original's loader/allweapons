                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[13]["Visi-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[13]["Visi-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[13]["Visi-Minimum hit chance"]))
            end
        end
    end
    end
    client.set_event_callback("setup_command", wep_set_value12)
    
    function wep_set_value13()
        if set_value_weapon() == "CWeaponTaser"then
            ui.set(target_selection_ref,ui.get(weapon_name[14]["Target selection"]))
            ui.set(multi_point_mode_ref,ui.get(weapon_name[14]["Multi-point_mode"]))
            ui.set(prefer_safe_point_ref,ui.get(weapon_name[14]["Prefer safe point"]))
            ui.set(quick_stop_ref,ui.get(weapon_name[14]["quick stop options"]))
            ui.set(Automatic_scope_ref,ui.get(weapon_name[14]["Automatic scope"]))
            ui.set(Force_body_aim_on_peek_ref,ui.get(weapon_name[14]["Force body aim on peek"]))
            if  damage[0] == 1 then
                ui.set(target_hitbox_ref,ui.get(weapon_name[14]["Mini-Target hitbox"]))
                ui.set(multi_point_ref,ui.get(weapon_name[14]["Mini-Multi-point"]))
                ui.set(multi_point_scale_ref,ui.get(weapon_name[14]["Mini-Multi-point scale"]))
                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[14]["Mini-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[14]["Mini-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[14]["Mini-Minimum hit chance"]))
        elseif damage[0] == 2 then
                ui.set(target_hitbox_ref,ui.get(weapon_name[14]["Doub-Target hitbox"]))
                ui.set(multi_point_ref,ui.get(weapon_name[14]["Doub-Multi-point"]))
                ui.set(multi_point_scale_ref,ui.get(weapon_name[14]["Doub-Multi-point scale"]))
                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[14]["Doub-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[14]["Doub-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[14]["Doub-Minimum hit chance"]))
        else
            if g==0 then
                ui.set(target_hitbox_ref,ui.get(weapon_name[14]["Auto-Target hitbox"]))
                ui.set(multi_point_ref,ui.get(weapon_name[14]["Auto-Multi-point"]))
                ui.set(multi_point_scale_ref,ui.get(weapon_name[14]["Auto-Multi-point scale"]))
                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[14]["Auto-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[14]["Auto-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[14]["Auto-Minimum hit chance"]))
            else
                ui.set(target_hitbox_ref,ui.get(weapon_name[14]["Visi-Target hitbox"]))
                ui.set(multi_point_ref,ui.get(weapon_name[14]["Visi-Multi-point"]))
                ui.set(multi_point_scale_ref,ui.get(weapon_name[14]["Visi-Multi-point scale"]))
                ui.set(Avoid_unsafe_hitboxes_ref,ui.get(weapon_name[14]["Visi-Avoid unsafe hitboxes"]))
                ui.set(minimum_damage_ref,ui.get(weapon_name[14]["Visi-Minimum damage"]))
                ui.set(minimum_hit_chance_ref,ui.get(weapon_name[14]["Visi-Minimum hit chance"]))
            end
        end
    end
    end
    client.set_event_callback("setup_command", wep_set_value13)
